type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetPasswordToken: String @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  forms: [Form]
}

type Form {
  id: ID! @unique
  name: String!
  logos: [Logo!]
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User!
  submissions: [Submission]
}

type Submission {
  id: ID! @unique
  name: String!
  logos: [Logo!]
  createdAt: DateTime!
  updatedAt: DateTime!
  form: Form!
}

type Logo {
  id: ID! @unique
  assetId: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User
}
